//Linear search in array
#include <stdio.h>
#define n 10
int main()
{
    int a,i,x,arr[n];
    printf("Enter the values:");
    for(i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    printf("Enter the element to be searched:");
    scanf("%d",&x);
    for(i=0;i<n;i++)
    {
        if(arr[i]==x)
        {
            printf("Element found at index %d",(i+1));
            break;
        }
    }
}
